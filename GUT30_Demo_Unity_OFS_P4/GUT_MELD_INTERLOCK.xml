<?xml version="1.0" encoding="utf-8"?>
<template desc="Equipment template">
  <param name="type" desc="equipment type parameters">
    <string name="name">GUT_MELD_INTERLOCK</string>
    <string name="ref">GUT_MELD_INTERLOCK</string>
 </param> 
